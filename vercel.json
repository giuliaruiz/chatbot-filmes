{
    "version": 2,
    "builds": [
      {
        "src": "api/chatbot.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/api/chatbot", "dest": "api/chatbot.js" },
      { "src": "/(.*)", "dest": "/public/$1" }
    ]
  }
  